{
  "assets": {
    "buildDir": "{{ himation_current_version_build_dir }}",
    "debug": {{ himation_debug_assets | lower }},
    "distDir": "{{ himation_current_version_assets_dir }}",
    "optimize": {{ himation_optimize_assets | lower }}
  },
  "server": {
    "debugLogging": {{ himation_use_debug_logging | lower }}
  },
  "servers": {
    "app": {
      "host": "{{ himation_app_server_host }}",
      "path": "{{ himation_app_server_path }}",
      "port": {{ himation_app_server_port }},
      "protocol": "{{ himation_app_server_protocol }}"
    },
    "assets": {
      "host": "{{ himation_asset_server_host }}",
      "path": "{{ himation_asset_server_path }}",
      "port": {{ himation_asset_server_port }},
      "protocol": "{{ himation_asset_server_protocol }}"
    }
  }
}
